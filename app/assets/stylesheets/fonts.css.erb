@font-face {
  font-family: 'LatoRegular';
  src: url('<%= asset_path("Lato-Reg-webfont.eot") %>');
  src: url('<%= asset_path("Lato-Reg-webfont.eot?#iefix") %>') format('embedded-opentype'),
       url('<%= asset_path("Lato-Reg-webfont.woff") %>') format('woff'),
       url('<%= asset_path("Lato-Reg-webfont.ttf") %>') format('truetype'),
       url('<%= asset_path("Lato-Reg-webfont.svg#LatoRegular") %>') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'OpenSansRegular';
  src: url('<%= asset_path("OpenSans-Regular-webfont.eot") %>');
  src: url('<%= asset_path("OpenSans-Regular-webfont.eot?#iefix") %>') format('embedded-opentype'),
       url('<%= asset_path("OpenSans-Regular-webfont.woff") %>') format('woff'),
       url('<%= asset_path("OpenSans-Regular-webfont.ttf") %>') format('truetype'),
       url('<%= asset_path("OpenSans-Regular-webfont.svg#open_sansregular") %>') format('svg');
  font-weight: normal;
  font-style: normal;
}

